journal = {IEEE Transactions on Power Electronics},
keywords = {Converter losses model,closed-loop control strategy,dc/dc boost converters,digital signal processor (DSP),direct digital control (DDC),fuel cell hybrid electric Vehicles (FCHEVs),generalized small-signal model},
number = {11},
pages = {4445--4458},
title = {{Analysis, modeling, and implementation of a multidevice interleaved DC/DC converter for fuel cell hybrid electric vehicles}},
volume = {27},
year = {2012}
}
