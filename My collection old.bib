journal = {Ieee},
keywords = {bryan c,department of mechanical engineering,direct-drive dc motor,linear actuator,magnet motor,murphy,permanent-,real-time digital control,student member,tubular motor},
pages = {1664--1671},
title = {{Development of a Novel Direct-Drive Tubular Linear Brushless Permanent-Magnet Motor}},
year = {2003}
}
