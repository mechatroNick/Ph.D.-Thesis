journal = {IEEE Transactions on Industrial Electronics},
keywords = {Control systems,Friction,Motion control},
number = {10},
pages = {3848--3853},
title = {{Friction compensation of an XY feed table using friction-model-based feedforward and an inverse-model-based disturbance observer}},
volume = {56},
year = {2009}
}
