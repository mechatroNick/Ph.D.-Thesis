journal = {Mechatronics},
keywords = {Adaptive control,Linear motor,Precision motion control,Robust control,Servo control},
number = {4},
pages = {595--616},
title = {{Adaptive robust motion control of linear motors for precision manufacturing}},
volume = {12},
year = {2002}
}
