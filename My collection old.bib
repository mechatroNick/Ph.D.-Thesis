@article{Encica2007,
abstract = {Engineering optimization procedures employ highly accurate numerical$\backslash$nmodels that typically have an excessive computational cost, e.g.,$\backslash$nfinite elements (FE). The space mapping (SM) technique speeds up$\backslash$nthe minimization procedure by exploiting simplified (less accurate)$\backslash$nmodels. We will use the SM terminology of fine and coarse to refer$\backslash$nto the accurate and inaccurate models, respectively. SM implementation$\backslash$nin the field of electromagnetic actuators design, in the context$\backslash$nof constrained optimization, is a rather unexplored topic. A linear$\backslash$nvoice coil actuator is chosen as a benchmark test example. The key$\backslash$nelement in SM is the so-called SM function, which efficiently corrects$\backslash$nthe imprecise results that can be obtained with just coarse information.$\backslash$nSM is used to solve a shape optimization problem. The design problem$\backslash$nis stated as a minimization in which the computational cost lies$\backslash$ncompletely in the constraint evaluation; thus, SM is applied only$\backslash$nto the constraints. A mathematical description of the approach is$\backslash$npresented in this paper and two implementations are compared. The$\backslash$nsolution of the SM optimization is validated (locally) by means of$\backslash$na standard minimization routine. The numerical results obtained show$\backslash$nhigh efficiency of the SM-based optimization algorithm, reflected$\backslash$nby a significantly low number of fine model simulations and an overall$\backslash$nlow computational effort.},
author = {Encica, L. and Echeverr{\'{i}}a, D. and Lomonova, E. A. and Vandenput, A. J A and Hemker, P. W. and Lahaye, D.},
doi = {10.1007/s00158-006-0054-6},
