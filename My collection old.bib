@article{Jahn1996,
abstract = {Permanent magnet AC (PMAC) motor drives are finding expanded use$\backslash$nin high-performance applications where torque smoothness is essential.$\backslash$nThis paper reviews a wide range of motor- and controller-based design$\backslash$ntechniques that have been described in the literature for minimizing the$\backslash$ngeneration of cogging and ripple torques in both sinusoidal and$\backslash$ntrapezoidal PMAC motor drives. Sinusoidal PMAC drives generally show the$\backslash$ngreatest potential for pulsating torque minimization using well-known$\backslash$nmotor design techniques such as skewing and fractional slot pitch$\backslash$nwindings. In contrast, trapezoidal PMAC drives pose more difficult$\backslash$ntrade-offs in both the motor and controller design which may require$\backslash$ncompromises in drive simplicity: and cost to improve torque smoothness.$\backslash$nController-based techniques for minimizing pulsating torque typically$\backslash$ninvolve the use of active cancellation algorithms which depend on either$\backslash$naccurate tuning or adaptive control schemes for effectiveness. In the$\backslash$nend, successful suppression of pulsating torque ultimately relies on an$\backslash$norchestrated systems approach to all aspects of the PMAC machine and$\backslash$ncontroller design which often requires a carefully selected combination$\backslash$nof minimization techniques},
author = {Jahns, Thomas M. and Soong, Wen L.},
doi = {10.1109/41.491356},
file = {:E$\backslash$:/Storage/Mendeley Folder/Jahns, Soong - 1996 - Pulsating torque minimization techniques for permanent magnet AC motor drives - A review(2).pdf:pdf},
isbn = {02780046},
issn = {02780046},
journal = {IEEE Transactions on Industrial Electronics},
number = {2},
pages = {321--330},
pmid = {19331493},
title = {{Pulsating torque minimization techniques for permanent magnet AC motor drives - A review}},
volume = {43},
year = {1996}
}
@article{Sheikh-Ghalavand2010,
abstract = {The aim of this work is to establish an accurate yet simple method for predicting flux density distribution and iron losses in linear permanent-magnet synchronous motors (LPMSMs) for iterative design procedures. For this purpose, an improved magnetic equivalent circuit for calculation of the teeth and yoke flux densities in the LPMSMs is presented. The magnetic saturation of iron core is considered by nonlinear elements and an iterative procedure is used to update these elements. The armature reaction is also taken into account in the modeling by flux sources located on the teeth of motors. These sources are time dependent and can model every winding configuration. The relative motion between the motor primary and secondary is considered by wisely designing air gap elements simplifying the permeance network construction and preventing permeance matrix distortion during primary motion. Flux densities in different load conditions are calculated by means of the proposed model. The effects of saturation and armature reaction on the flux density distribution are shown in detail. Using these flux densities, iron losses in the motor are examined and its variations versus motor parameters are then studied. All results obtained by proposed model are verified by finite-element method based on an extensive analysis.},
author = {Sheikh-Ghalavand, B. and Vaez-Zadeh, S. and {Hassanpour Isfahani}, A.},
doi = {10.1109/TMAG.2009.2030674},
file = {:E$\backslash$:/Storage/Mendeley Folder/Sheikh-Ghalavand, Vaez-Zadeh, Hassanpour Isfahani - 2010 - An improved magnetic equivalent circuit model for iron-core linear permane(2).pdf:pdf},
isbn = {0018-9464 VO  - 46},
issn = {00189464},
journal = {IEEE Transactions on Magnetics},
keywords = {Armature reaction,Finite-element method,Iron loss,Linear permanent-magnet synchronous motor,Magnetic equivalent circuit,Saturation},
number = {1},
pages = {112--120},
title = {{An improved magnetic equivalent circuit model for iron-core linear permanent-magnet synchronous motors}},
volume = {46},
year = {2010}
}
