author = {Badstuebner, Uwe and Biela, Juergen and Christen, Daniel and Kolar, Johann W.},
doi = {10.1109/TIE.2010.2103536},
file = {:E$\backslash$:/Storage/Mendeley Folder/Badstuebner et al. - 2011 - Optimization of a 5-kW telecom phase-shift dc-dc converter with magnetically integrated current doubler.pdf:pdf},
issn = {02780046},
journal = {IEEE Transactions on Industrial Electronics},
keywords = {{\#}compareConverterOptimization,Analytical model,dc-dc converter,design process,integrated current doubler,magnetic snubber,optimization},
mendeley-tags = {{\#}compareConverterOptimization},
