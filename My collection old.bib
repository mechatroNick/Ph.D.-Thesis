@article{Todorovic2008,
abstract = {In this paper, an analysis and design of a wide input range dc-dc$\backslash$nconverter is proposed along with a robust power control scheme. The$\backslash$nproposed converter and its control are designed to be compatible$\backslash$nwith a fuel cell power source, which exhibits 2 : 1 voltage variation$\backslash$nas well as a slow transient response. The proposed approach consists$\backslash$nof two stages: a three-level boost converter stage cascaded with$\backslash$na current-fed two-inductor boost converter topology, which has a$\backslash$nhigher voltage gain and provides galvanic isolation from the input$\backslash$nsource. The function of the front-end boost converter stage is to$\backslash$nmaintain a constant voltage at the input of the cascaded dc-dc converter$\backslash$nto ensure optimal performance characteristics and high efficiency.$\backslash$nAt the output of the first boost converter, a battery or ultracapacitor$\backslash$nenergy storage is connected to handle slow transient response of$\backslash$nthe fuel cell (200 W/min). The robust features of the proposed control$\backslash$nsystem ensure a constant output dc voltage for a variety of load$\backslash$nfluctuations, thus limiting the power being delivered by the fuel$\backslash$ncell during a load transient. Moreover, the proposed configuration$\backslash$nsimplifies power management and can interact with the fuel cell controller.$\backslash$nSimulation and the experimental results confirm the feasibility of$\backslash$nthe proposed system.},
author = {Todorovic, M H and Palma, L and Enjeti, P N},
doi = {10.1109/TIE.2007.911200},
file = {:E$\backslash$:/Storage/Mendeley Folder/Todorovic, Palma, Enjeti - 2008 - DC Converter With a Robust Power Control Scheme Suitable for Fuel Cell Power Conversion.pdf:pdf},
isbn = {0278-0046 VO - 55},
issn = {0278-0046},
journal = {Industrial Electronics, IEEE Transactions on},
keywords = {Batteries;DC-DC power converters;Fuel cells;Galvan},
number = {3},
pages = {1247--1255},
title = {{DC Converter With a Robust Power Control Scheme Suitable for Fuel Cell Power Conversion}},
volume = {55},
year = {2008}
}
@article{Kim2004,
abstract = { In this paper, a new active clamping current-fed half-bridge converter has been proposed, which is suitable for fuel cell based power generation systems. The proposed converter has outstanding advantages over the conventional dc-dc converters with respect to high efficiency, high power density, and high component utilization. In special, the proposed converter has predominant high boosting output voltage and high efficiency characteristics under the inherently severe low output voltage of the fuel cell through the overall load conditions. Moreover, the developed converter has been experimentally tested with the help of a fuel cell simulator, which is developed by Sungkyunkwan Univ. and can generate the V-I characteristics of polymer electrolyte membrane fuel cell (PEMFC), so that the performance of the proposed converter could be effectively examined and the validity of the converter could be verified.},
