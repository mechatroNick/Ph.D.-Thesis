isbn = {1376689936},
issn = {0307904X},
journal = {Applied Mathematical Modelling},
keywords = {Active disturbance rejection control (ADRC),Hybrid cascaded H-bridge (HCHB),Hybrid frequency carrier-based PWM (HFPWM),Reference trajectory optimization (RTO),Voice coil motor (VCM)},
