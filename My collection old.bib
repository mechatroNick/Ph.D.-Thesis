journal = {Conference Proceedings - IEEE Applied Power Electronics Conference and Exposition - APEC},
keywords = {{\#}compareConverterOptimization},
mendeley-tags = {{\#}compareConverterOptimization},
pages = {1838--1845},
title = {{Multi-objective design optimization of renewable energy system inverters using a Descriptive language for the components}},
year = {2011}
}
