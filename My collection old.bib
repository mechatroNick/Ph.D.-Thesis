keywords = {Constant-power loads (CPLs),Control,DC/DC converters,Feedback linearization,Multiconverter power electronic systems,Negative impedance instability,Vehicular power systems},
number = {2},
pages = {650--661},
title = {{Loop-cancellation technique: A novel nonlinear feedback to overcome the destabilizing effect of constant-power loads}},
volume = {59},
year = {2010}
}
