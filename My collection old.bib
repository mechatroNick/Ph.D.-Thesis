journal = {IEEE Transactions on Magnetics},
keywords = {Electric machines,Fourier analysis,finite permeability,semi-analytical,slotted},
number = {2},
title = {{Magnetic Saturation in Semi-Analytical Harmonic Modeling for Electric Machine Analysis}},
volume = {52},
year = {2016}
}
