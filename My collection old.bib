author = {{Won-jong Kim}, Bryan C. Murphy},
doi = {0-7803-7883-0/03/$17.00 © 2003 IEEE},
file = {:E$\backslash$:/Storage/Mendeley Folder/Won-jong Kim - 2003 - Development of a Novel Direct-Drive Tubular Linear Brushless Permanent-Magnet Motor(2).pdf:pdf},
isbn = {0-7803-7883-0},
