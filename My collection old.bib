keywords = {Eddy currents,Linear motors,Losses,Permanent-magnet machines},
number = {4},
pages = {761--770},
title = {{Analytical prediction of eddy-current loss in modular tubular permanent-magnet machines}},
volume = {20},
year = {2005}
}
