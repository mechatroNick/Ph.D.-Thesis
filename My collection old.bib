pages = {2340--2349},
title = {{Control of Cascaded DC-DC Converter Based Hybrid Battery Energy Storage Systems: Part I: Stability Issue}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=7359163},
volume = {63},
year = {2015}
}
