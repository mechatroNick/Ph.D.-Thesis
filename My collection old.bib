journal = {IECON Proceedings (Industrial Electronics Conference)},
keywords = {gallium nitride transistor,power electronic devices,switching energy measurement,wide bandgap devices},
pages = {246--251},
title = {{A non-intrusive method for measuring switching losses of GaN power transistors}},
year = {2013}
}
