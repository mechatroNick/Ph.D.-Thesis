@article{McFarland2015,
abstract = {This paper investigates the principles underlying torque production in a flux-switching permanent magnet (FSPM) machine. Because the phase windings and permanent magnets in FSPM machines are both located on the stator, the torque production mechanism is not the same as for a conventional PM synchronous machine. Spatial harmonic analysis is applied to examine the frequency components present in the electric loading and magnetic loading of the machine. Since torque is proportional to the product of the electric and magnetic loading, understanding the source of the principal harmonics in these waveforms yields powerful insights into the components that result in torque production. The analysis is first presented for a specific FSPM machine (12-slot, 10-pole) and then extended to a general FSPM machine. The primary torque-producing harmonics in the airgap flux density waveform are found to be the heterodyned harmonics of the MMF produced by the stator magnets and the airgap permeance seen by the stator looking into the rotor. Analytical results are compared to results from finite element (FE) analysis and exhibit good agreement.},
author = {McFarland, James D. and Jahns, Thomas M. and El-Refaie, Ayman M.},
doi = {10.1109/TIA.2015.2411655},
file = {:E$\backslash$:/Storage/Mendeley Folder/McFarland2015.pdf:pdf},
isbn = {9781479956982},
issn = {00939994},
journal = {IEEE Transactions on Industry Applications},
keywords = {Finite-element analysis,harmonic analysis,modulation,permanent-magnet machines,permeability,torque,traction motors},
number = {4},
pages = {3041--3049},
publisher = {IEEE},
title = {{Analysis of the Torque Production Mechanism for Flux-Switching Permanent-Magnet Machines}},
volume = {51},
year = {2015}
}
