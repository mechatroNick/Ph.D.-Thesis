@article{kim2003,
abstract = {Abstract—This paper presents a novel design for a tubular$\backslash$r$\backslash$nlinear brushless permanent-magnet motor. In this design, the$\backslash$r$\backslash$nmagnets in the moving part are oriented in an NS-NS—SN-SN$\backslash$r$\backslash$nfashion which leads to higher magnetic force near the like-pole$\backslash$r$\backslash$nregion. An analytical methodology to calculate the motor force$\backslash$r$\backslash$nand to size the actuator was developed. The linear motor is$\backslash$r$\backslash$noperated in conjunction with a position sensor, three power$\backslash$r$\backslash$namplifiers, and a controller to form a complete solution for$\backslash$r$\backslash$ncontrolled precision actuation. Real-time digital controllers$\backslash$r$\backslash$nenhanced the dynamic performance of the motor, and gain$\backslash$r$\backslash$nscheduling reduced the effects of a nonlinear dead band. In its$\backslash$r$\backslash$ncurrent state, the motor has a rise time of 30 ms, a settling time of$\backslash$r$\backslash$n60 ms, and 25{\%} overshoot to a 5-mm step command. The motor$\backslash$r$\backslash$nhas a maximum speed of 1.5 m/s and acceleration up to 10 g. It$\backslash$r$\backslash$nhas a 10-cm travel range and 26-N maximum pull-out force. The$\backslash$r$\backslash$ncompact size of the motor suggests it could be used in robotic$\backslash$r$\backslash$napplications requiring moderate force and precision, such as$\backslash$r$\backslash$nrobotic-gripper positioning or actuation. The moving part of the$\backslash$r$\backslash$nmotor can extend significantly beyond its fixed support base.$\backslash$r$\backslash$nThis reaching ability makes it useful in applications requiring a$\backslash$r$\backslash$nsmall, direct-drive actuator, which is required to extend into a$\backslash$r$\backslash$nspatially constrained environment.},
author = {{Won-jong Kim}, Bryan C. Murphy},
doi = {0-7803-7883-0/03/$17.00 © 2003 IEEE},
file = {:E$\backslash$:/Storage/Mendeley Folder/Won-jong Kim - 2003 - Development of a Novel Direct-Drive Tubular Linear Brushless Permanent-Magnet Motor(2).pdf:pdf},
isbn = {0-7803-7883-0},
issn = {15986446},
journal = {Ieee},
keywords = {bryan c,department of mechanical engineering,direct-drive dc motor,linear actuator,magnet motor,murphy,permanent-,real-time digital control,student member,tubular motor},
pages = {1664--1671},
title = {{Development of a Novel Direct-Drive Tubular Linear Brushless Permanent-Magnet Motor}},
year = {2003}
}
@article{Gysen2008c,
abstract = {We present analytical and numerical methods for determining the magnetic field distribution in a tubular permanent-magnet actuator (TPMA). In Part I, we present the semianalytical method. This method has the advantage of a relatively short computation time and it gives physical insight. We make an extension for skewed topologies, which offer the benefit of reducing the large force ripples of the TPMA. However, a lot of assumptions and simplifications with respect to the slotted structure have to be made in order to come to a relatively simple semianalytical description. To model the slotting effect and the related cogging force, we apply a Schwarz-Christoffel (SC) mapping for magnetic field and force calculations in Part II of the paper. Validation of the models is done with finite-element analysis.},
